        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Insurance"
        xmlns:dxe="http://schemas.devexpress.com/winfx/2008/xaml/editors" x:Name="SettGlobal" x:Class="Insurance.SettGLOB"
        mc:Ignorable="d"
        Title="SettGLOB" Height="130.81" Width="587.945">
    <Window.Background>
        <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
            <GradientStop Color="Black" Offset="0"/>
            <GradientStop Color="#FF5F7B8B" Offset="0.563"/>
        </LinearGradientBrush>
    </Window.Background>
    <Grid>
        <CheckBox x:Name="Wigruzka_tfoms" Content="Автоматическая выгрузка в ТФОМС" HorizontalAlignment="Left" Margin="18,24,0,0" VerticalAlignment="Top" Foreground="White" Height="31" Width="416" FontSize="22" Checked="Wigruzka_tfoms_Checked"/>
        <CheckBox x:Name="FIAS_ONLINE" Content="Использовать FIAS ONLINE" HorizontalAlignment="Left" Margin="18,60,0,0" VerticalAlignment="Top" FontSize="22" Foreground="White" Checked="FIAS_ONLINE_Checked"/>
    </Grid>
</Window>
